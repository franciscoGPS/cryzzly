module TimeToNumber
  struct Time

    def to_i8
      0.as(Int8)
    end
  
    def to_i16
      0.as(Int16)
    end
  
    def to_i32
      0.as(Int32)
    end
  
    def to_i64
      0.as(Int64)
    end
  
    def to_f64
      0.as(Float64)
    end
  
    def to_f32
      0.as(Float32)
    end
  end
end